<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            id="JrubyEclipsePlugin"
            name="Jruby Eclipse">
      </category>
      <view
            category="JrubyEclipsePlugin"
            class="com.restphone.jrubyeclipse.RubyOutputView"
            icon="icons/sample.gif"
            id="com.restphone.jrubyeclipse.RubyOutputView2"
            name="Ruby Output">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="com.restphone.jrubyeclipse.RubyOutputView2"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="TestePlugin.contribution1"
            objectClass="org.eclipse.jdt.core.IJavaProject">
       <menu
             icon="icons/Binoculars.png"
             id="TestePlugin.menu2"
             label="Knowledge Discovery"
             path="additions">
            <groupMarker
                  name="content">
            </groupMarker>
            <separator
                  name="additions">
            </separator>
         </menu>
       <action
             class="com.br.actions.IdentifySQL"
             enablesFor="1"
             icon="icons/1378475458_stock_data-edit-sql-query.png"
             id="com.actions.newAction"
             label="Discovery SQL MODEL"
             menubarPath="TestePlugin.menu2/content">
     </action>
       <action
             class="com.br.actions.IdentifyJavaModel"
             enablesFor="1"
             icon="icons/1378475440_Java.png"
             id="com.actions.newActionJava"
             label="Discovery Java MODEL"
             menubarPath="TestePlugin.menu2/content">
       </action>
       <action
             class="com.br.actions.IdentifyKDMModel"
             enablesFor="1"
             icon="icons/4328.png"
             id="com.actions.newActionKDMModel"
             label="Discovery KDM MODEL"
             menubarPath="TestePlugin.menu2/content">
       </action>
       <action
             class="com.br.actions.IdentifyKDMSource"
             enablesFor="1"
             icon="icons/4328.png"
             id="com.actions.newActionKDMSOURCE"
             label="Discovery KDM SOURCE"
             menubarPath="TestePlugin.menu2/content">
       </action>
      </objectContribution>
   </extension>
	<extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="TestePlugin.contribution2"
            nameFilter="*.sqlmodel"
            objectClass="org.eclipse.core.resources.IResource">
         <menu
               icon="icons/4188.png"
               id="TestePlugin.menu2"
               label="TransformSQLModel2KDMData"
               path="additions">
            <groupMarker
                  name="content">
            </groupMarker>
            <separator
                  name="additions">
            </separator>
         </menu>
         <action
               class="com.br.actions.IdentifyKDMData"
               enablesFor="1"
               icon="icons/1378475289_54.png"
               id="TestePlugin.newAction"
               label=" KDM Data"
               menubarPath="TestePlugin.menu2/content">
         </action>
      </objectContribution>
   </extension>
 <extension
       point="org.eclipse.ui.popupMenus">
    <objectContribution
          adaptable="false"
          id="TestePlugin.contribution3"
          objectClass="org.eclipse.jdt.core.IJavaProject">
       <menu
             icon="icons/5124.png"
             id="TestePlugin.menu3"
             label="Modernization"
             path="additions">
          <groupMarker
                name="content">
          </groupMarker>
          <separator
                name="additions">
          </separator>
       </menu>
       <action
             class="com.br.actions.ReestructuringToDAO"
             enablesFor="1"
             icon="icons/4416.png"
             id="com.actions.newActionReestructuring"
             label="Modernize2DAO"
             menubarPath="TestePlugin.menu3/content">
       </action>
    </objectContribution>
 </extension>
 <extension
       point="org.eclipse.ui.views">
    <view
          class="com.br.gui.refactoring.RefactoringNameClass"
          id="org.eclipse.wb.swt.RefactoringNameClass"
          name="name"
          restorable="true">
    </view>
    <view
          class="com.br.gui.refactoring.RenameClassUnit"
          id="com.br.gui.refactoring.RenameClassUnit"
          name="Rename ClassUnit">
    </view>
 </extension>
 <extension
       point="org.eclipse.ui.popupMenus">
    <objectContribution
          adaptable="false"
          id="TestePlugin.contribution3"
          objectClass="org.eclipse.ui.IEditorInput">
       <menu
             icon="icons/5124.png"
             id="TestePlugin.menu4"
             label="Refactoring KDM"
             path="additions">
          <groupMarker
                name="content">
          </groupMarker>
          <separator
                name="additions">
          </separator>
       </menu>
       <action
             class="com.br.actions.ActionRefactoringRenameClass"
             enablesFor="1"
             icon="icons/4416.png"
             id="com.actions.newActionRefactoringName"
             label="Rename Class"
             menubarPath="TestePlugin.menu4/content">
       </action>
    </objectContribution>
 </extension>
</plugin>
